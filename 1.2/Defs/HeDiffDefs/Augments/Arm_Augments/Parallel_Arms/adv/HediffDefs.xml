<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <HediffDef ParentName="AddedBodyPartBase">
    <defName>ParallelArm</defName>
    <label>Parallel arms</label>
    <labelNoun>a set of parallel arms</labelNoun>
    <description>A set of parallel arms. Great for fine details, but more fragile.</description>
    <spawnThingOnRemoved>ParallelArm</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>2.5</partEfficiency>
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <stages>
      <li>
        <statOffsets>
          <ArmorRating_Sharp>-1.0</ArmorRating_Sharp>
          <ArmorRating_Blunt>-1.0</ArmorRating_Blunt>
          <MaxHitPoints>-20</MaxHitPoints>
        </statOffsets>
      </li>
    </stages>
  </HediffDef>
</Defs>